<window>
	<defaultcontrol>200</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<animation effect="fade" time="250">WindowOpen</animation>
	<animation effect="fade" time="250">WindowClose</animation>
	<controls>
		<control type="group">
			<posx>0</posx>
			<posy>0</posy>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<texture border="0">backdrop.png</texture>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>290</posy>
				<width>1280</width>
				<height>120</height>
				<texture border="0">defaultPanel.png</texture>
			</control>	
			<control type="button" id="230">
				<description>Close Window button</description>
				<posx>1210</posx>
				<posy>410</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>120</onleft>
				<onright>120</onright>
				<onup>120</onup>
				<ondown>120</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<!-- Stubs for image overlays -->
			<control type="image" id="210"><posx>50</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="211"><posx>230</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="212"><posx>410</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="213"><posx>590</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="214"><posx>770</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="215"><posx>950</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="216"><posx>1130</posx><posy>300</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="217"><posx>0</posx><posy>0</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="218"><posx>0</posx><posy>0</posy><width>100</width><height>100</height><fadetime>1</fadetime></control>
			<control type="image" id="220">
				<posx>590</posx>
				<posy>390</posy>
				<width>100</width>
				<height>20</height>
				<texture border="0">selector.png</texture>
			</control>	
		</control>
	</controls>
</window>
