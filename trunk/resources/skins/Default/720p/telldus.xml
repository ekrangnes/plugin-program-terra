<window>
	<defaultcontrol>200</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<animation effect="fade" time="250">WindowOpen</animation>
	<animation effect="fade" time="250">WindowClose</animation>
	<controls>
		<control type="group">
			<posx>0</posx>
			<posy>0</posy>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>300</width>
				<height>720</height>
				<texture border="0">defaultPanel.png</texture>
			</control>
			<control type="button" id="200">
				<description>Close Window button</description>
				<posx>20</posx>
				<posy>0</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>120</onleft>
				<onright>120</onright>
				<onup>120</onup>
				<ondown>120</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<!-- Stubs for image overlays -->
			<control type="image" id="210"><posx>15</posx><posy>85</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="211"><posx>15</posx><posy>160</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="212"><posx>15</posx><posy>235</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="213"><posx>15</posx><posy>310</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="214"><posx>15</posx><posy>385</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="215"><posx>15</posx><posy>460</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="216"><posx>15</posx><posy>535</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
			<control type="image" id="217"><posx>15</posx><posy>610</posy><width>59</width><height>59</height><fadetime>1</fadetime></control>
		</control>
	</controls>
</window>
